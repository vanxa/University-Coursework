include "m8c.inc"


;--------------------------------------------------
; Constants for Counter8_DC API's.
;--------------------------------------------------

Counter8_DC_CONTROL_REG_START_BIT:         equ 0x01    ; Control register start bit
Counter8_DC_INT_REG:                       equ 0x0df
Counter8_DC_INT_MASK:                      equ 0x10


;--------------------------------------------------
; Constants for Counter8_DC user defined values
;--------------------------------------------------

Counter8_DC_PERIOD:                        equ 0x08
Counter8_DC_COMPARE_VALUE:                 equ 0x08


;--------------------------------------------------
; Register Address Constants for Counter8_DC
;--------------------------------------------------

Counter8_DC_COUNTER_REG:               equ 0x50   ; DR0 Counter register
Counter8_DC_PERIOD_REG:                equ 0x51   ; DR1 Period register
Counter8_DC_COMPARE_REG:               equ 0x52   ; DR2 CompareValue register
Counter8_DC_CONTROL_REG:               equ 0x53   ; Control register
Counter8_DC_FUNC_REG:                  equ 0x50   ; Function register
Counter8_DC_INPUT_REG:                 equ 0x51   ; Input register
Counter8_DC_OUTPUT_REG:                equ 0x52   ; Output register


;--------------------------------------------------
; Counter8_DC Macro 'Functions'
;--------------------------------------------------

   macro Counter8_DC_Start_M
   or    reg[Counter8_DC_CONTROL_REG],  Counter8_DC_CONTROL_REG_START_BIT
   endm

   macro Counter8_DC_Stop_M
   and   reg[Counter8_DC_CONTROL_REG], ~Counter8_DC_CONTROL_REG_START_BIT
   endm

   macro Counter8_DC_EnableInt_M
   M8C_EnableIntMask Counter8_DC_INT_REG, Counter8_DC_INT_MASK
   endm

   macro Counter8_DC_DisableInt_M
   M8C_DisableIntMask Counter8_DC_INT_REG, Counter8_DC_INT_MASK
   endm


